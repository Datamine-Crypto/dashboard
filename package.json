{
	"name": "datamine-network-dashboard",
	"version": "1.0.0",
	"private": true,
	"homepage": ".",
	"description": "A web-based dashboard for the Datamine Network, built with React and TypeScript, providing tools to interact with Datamine smart contracts, view analytics, and manage assets across different blockchain layers.",
	"keywords": [
		"datamine",
		"flux",
		"blockchain",
		"react",
		"typescript",
		"web3",
		"dashboard",
		"defi",
		"ethereum",
		"arbitrum",
		"material-ui"
	],
	"author": "Datamine Network",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Datamine-Crypto/dashboard.git"
	},
	"bugs": {
		"url": "https://github.com/Datamine-Crypto/dashboard/issues"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@fontsource-variable/roboto": "^5.2.9",
		"@metamask/detect-provider": "^2.0.0",
		"@mui/icons-material": "^7.3.5",
		"@mui/material": "^7.3.5",
		"@mui/system": "^7.3.5",
		"@mui/utils": "^7.3.5",
		"@mui/x-date-pickers": "^8.17.0",
		"big.js": "^7.0.1",
		"bn.js": "^5.2.2",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.19",
		"events": "^3.3.0",
		"fuse.js": "^7.1.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-markdown": "^10.1.0",
		"tss-react": "^4.9.19",
		"uuid": "^13.0.0",
		"viem": "^2.40.0",
		"zustand": "^5.0.8"
	},
	"scripts": {
		"start": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"eject": "echo 'Vite does not require an eject script.'",
		"deploy": "vite build --base=./",
		"bundle-visualizer": "npx vite-bundle-visualizer",
		"format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,scss,md}",
		"lint": "eslint .",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"prepare": "husky"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}": [
			"yarn eslint --fix",
			"yarn prettier --write"
		]
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.39.1",
		"@types/big.js": "^6.2.2",
		"@types/bn.js": "^5.2.0",
		"@types/events": "^3.0.3",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.10.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@types/react-vis": "^1.11.15",
		"@types/uuid": "^11.0.0",
		"@vitejs/plugin-react": "^5.1.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-unused-imports": "^4.3.0",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.6",
		"prettier": "^3.6.2",
		"typescript": "5.9.3",
		"typescript-eslint": "^8.46.4",
		"vconsole": "^3.15.1",
		"vite": "^7.2.2",
		"vite-bundle-visualizer": "^1.2.1",
		"vite-plugin-mkcert": "^1.17.9"
	},
	"packageManager": "yarn@4.10.3"
}
